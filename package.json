{
  "name": "better-bot",
  "version": "1.0.0",
  "description": "A discord bot better than the last one.",
  "main": "./dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "tsc && ts-node-dev ./src/index.ts",
    "startdev": "nodemon",
    "startauto": "ts-node-dev --respawn --transpile-only --poll ./src/index.ts",
    "deploy": "ts-node-dev --respawn --transpile-only --poll ./src/deploy/global.ts",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danloe/Better-Bot.git"
  },
  "keywords": [
    "discord",
    "js",
    "bot"
  ],
  "author": "danloe",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danloe/Better-Bot/issues"
  },
  "homepage": "https://github.com/danloe/Better-Bot#readme",
  "dependencies": {
    "@discordjs/builders": "^0.13.0",
    "@discordjs/opus": "^0.5.3",
    "@discordjs/rest": "^0.4.1",
    "@discordjs/voice": "^0.9.0",
    "discord-api-types": "^0.31.2",
    "discord-modals": "^1.3.5",
    "discord-tts": "^1.2.1",
    "discord.js": "^13.6.0",
    "dotenv": "^16.0.0",
    "easy-trivia": "^2.1.5",
    "epic-free-games": "^3.1.1",
    "googlethis": "^1.2.9",
    "libsodium-wrappers": "^0.7.10",
    "mongoose": "^6.3.1",
    "sodium": "^3.0.2",
    "soundcloud-downloader": "^1.0.0",
    "ytdl-core": "^4.11.0",
    "ytsr": "^3.8.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.27",
    "eslint": "^8.14.0",
    "nodemon": "^2.0.16",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  }
}
